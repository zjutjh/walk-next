{
  "name": "walk-next",
  "version": "0.0.0",
  "description": "毅行前端 monorepo, 包含用户端，扫码端，大盘",
  "private": true,
  "packageManager": "pnpm@10.30.1",
  "engines": {
    "node": ">=24"
  },
  "author": "zjutjh",
  "scripts": {
    "dev:admin": "pnpm -C apps/admin dev",
    "build:admin": "pnpm -C apps/admin build",
    "lint": "eslint",
    "postinstall": "simple-git-hooks",
    "typecheck": "pnpm -r typecheck"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "catalog:infra",
    "@typescript-eslint/parser": "catalog:infra",
    "@zjutjh/eslint-config": "catalog:infra",
    "eslint": "catalog:infra",
    "eslint-config-prettier": "catalog:infra",
    "eslint-plugin-format": "catalog:infra",
    "eslint-plugin-prettier": "catalog:infra",
    "eslint-plugin-vue": "catalog:infra",
    "jiti": "catalog:infra",
    "lint-staged": "catalog:infra",
    "prettier": "catalog:infra",
    "simple-git-hooks": "catalog:infra",
    "vue-eslint-parser": "catalog:infra"
  }
}
